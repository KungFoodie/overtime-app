ent: '';\r\n    height: 100%;\r\n    left: -5000px;\r\n    position: absolute;\r\n    top: 0;\r\n    width: 10000px;\r\n    z-index: -2;\r\n}\r\n\r\ntd:hover::after {\r\n    background-color: rgba(255,255,255,0.2);\r\n    content: '';\r\n    height: 10000px;\r\n    left: 0;\r\n    position: absolute;\r\n    top: -5000px;\r\n    width: 100%;\r\n    z-index: -1;\r\n}\r\n\r\n.empty-db {\r\n    text-align: center;\r\n}\r\n\r\n/* Admin Page */\r\n\r\n.adminbuttonheader {\r\n    display: block;\r\n    width: 100%;\r\n}\r\n\r\n.admincol h1{\r\n    text-align: center;\r\n}\r\n\r\n.adminbuttons {\r\n    align-items: center;\r\n    justify-content: left;\r\n    width: 100%;\r\n\r\n}\r\n\r\n.adminbuttonheader {\r\n    text-align: center;\r\n}\r\n\r\n/* Forms */\r\n\r\nlegend {\r\n    font-size: larger;\r\n    font-weight: bolder;\r\n}\r\n\r\n.question {\r\n    font-weight: bold;\r\n    display: inline-block;\r\n    width: 300px;\r\n    text-align: left;\r\n}\r\n\r\n.textBox {\r\n    width: 250px;\r\n    border-radius: 1rem;\r\n    background-color: rgba(255,255,255,0.9);\r\n    border: 1px solid rgba(0,0,0,0.1);\r\n}\r\n\r\n.cancel {\r\n    float: right;\r\n}\r\n\r\n.select {\r\n    width: 250px;\r\n    border-radius: 1rem;\r\n    background-color: rgba(255,255,255,0.9);\r\n    border: 1px solid rgba(0,0,0,0.1);\r\n}\r\n\r\n.recordformcol .button, .employeeform .button {\r\n    width: 20%;\r\n}\r\n\r\nfieldset {\r\n    border-radius: 1rem;\r\n    border: 1px solid rgba(0,0,0,0.2);\r\n    text-align: center;\r\n}\r\n\r\nlegend {\r\n    font-weight: bold;\r\n    font-size: 30px;\r\n}\r\n\r\n.recordformcol, .employeeform{\r\n    width: 70%;\r\n    margin: auto;\r\n}\r\n\r\n.recordformcol fieldset, .employeeform fieldset {\r\n    margin: auto;\r\n    width: 90%;\r\n}\r\n\r\n.add {\r\n    float: left;\r\n}\r\n\r\n.save {\r\n    float: left;\r\n}\r\n\r\n.mod {\r\n    float: right;\r\n}\r\n\r\n/* Alert Box */\r\n#alert {\r\n    display: none;\r\n    position: relative;\r\n    width: auto;\r\n    height: auto;\r\n    padding: 10px;\r\n    margin: 10px;\r\n    border-radius: 1rem;\r\n    cursor: pointer;\r\n    border: 1px solid rgba(0,0,0,0.1);\r\n    background-color: rgba(100,100,100,0.8);\r\n    color: rgba(255,255,255,0.9);\r\n}\r\n\r\n.alertCheckbox {\r\n    display: none;\r\n}\r\n\r\n:checked + .alert {\r\n    display: none;\r\n}\r\n\r\n.alertText {\r\n    display: table;\r\n    margin: 0 auto;\r\n    text-align: center;\r\n    font-size: 16px;\r\n}\r\n\r\n.alertClose {\r\n    float: right;\r\n    padding-right: 10px;\r\n    font-size: 15px;\r\n}\r\n\r\n.clear {\r\n    clear: both;\r\n}\r\n\r\n.info {\r\n    background-color: #EEE;\r\n    border: 1px solid #DDD;\r\n    color: #999;\r\n}\r\n\r\n\r\n/* Footer */\r\n\r\nfooter {\r\n    position: absolute;\r\n    text-align: left;\r\n    bottom: 0;\r\n    left: 0;\r\n    padding: 10px;\r\n}\r\n\r\n@media screen and (max-width: 1200px) {\r\n    .column {\r\n        width: 100%;\r\n    }\r\n\r\n    label.question {\r\n        text-align: left;\r\n        width: 20%;\r\n    }\r\n\r\n    /* Footer */\r\n\r\n    footer {\r\n        position: relative;\r\n\r\n    }\r\n}\r\n
